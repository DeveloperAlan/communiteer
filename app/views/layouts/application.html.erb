<!DOCTYPE html>
<html>
<head>
  <title>Communiteer</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render partial: 'layouts/header' %>
  <div id="main">
